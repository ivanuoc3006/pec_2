@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#> .

<http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasEventType
:hasEventType rdf:type owl:ObjectProperty ;
              rdfs:domain :Events ;
              rdfs:range :EventTypes ;
              rdfs:comment "Relates an event with his type" ;
              rdfs:label "hasEventType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasHotelType
:hasHotelType rdf:type owl:ObjectProperty ;
              rdfs:domain :Hotels ;
              rdfs:range :HotelType ;
              rdfs:comment "Relates a hotel with his type" ;
              rdfs:label "hasHotelType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasPointOfInterestType
:hasPointOfInterestType rdf:type owl:ObjectProperty ;
                        rdfs:domain :PointsOfInteres ;
                        rdfs:range :PointOfInterestTypes ;
                        rdfs:comment "Relates a Point of interest with his type" ;
                        rdfs:label "hasPointOfInterestType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasRestaurantType
:hasRestaurantType rdf:type owl:ObjectProperty ;
                   rdfs:domain :Restaurants ;
                   rdfs:range :RestaurantTypes ;
                   rdfs:comment "Relates a restaurant with his type" ;
                   rdfs:label "hasRestaurantType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasTransportCompany
:hasTransportCompany rdf:type owl:ObjectProperty ;
                     rdfs:domain :Transport ;
                     rdfs:range :TransportCompanies ;
                     rdfs:comment "Relates a transport with its transport company" ;
                     rdfs:label "hasTransportCompany" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasTransportType
:hasTransportType rdf:type owl:ObjectProperty ;
                  rdfs:domain :Transport ;
                  rdfs:range :TransportTypes ;
                  rdfs:comment "Relates a transport with its type" ;
                  rdfs:label "hasTransportType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hotelRated
:hotelRated rdf:type owl:ObjectProperty ;
            rdfs:domain :Hotels ;
            rdfs:range :HotelRating ;
            rdfs:comment "Relates a hotel with his rating" ;
            rdfs:label "hasHotelRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#isCityOf
:isCityOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :takePlace ;
          rdfs:domain :City ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Events
                                     :Hotels
                                     :PointsOfInteres
                                     :Restaurants
                                     :Transport
                                   )
                     ] ;
          rdfs:comment "Indicates that is city of" ;
          rdfs:label "isCityOf" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#nightCost
:nightCost rdf:type owl:ObjectProperty ;
           rdfs:domain :Hotels ;
           rdfs:range :HotelNightCost ;
           rdfs:comment "Relates a hotel with his cost" ;
           rdfs:label "hasHotelNightCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#pointOfInterestRated
:pointOfInterestRated rdf:type owl:ObjectProperty ;
                      rdfs:domain :PointsOfInteres ;
                      rdfs:range :PointOfInterestRating ;
                      rdfs:comment "Relates a point with his rating" ;
                      rdfs:label "hasPoiRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#restaurantAverageCost
:restaurantAverageCost rdf:type owl:ObjectProperty ;
                       rdfs:domain :Restaurants ;
                       rdfs:range :RestaurantAverageCost ;
                       rdfs:comment "Relates a restaurant with his average cost" ;
                       rdfs:label "hasRestaurantAverageCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#restaurantRated
:restaurantRated rdf:type owl:ObjectProperty ;
                 rdfs:domain :Restaurants ;
                 rdfs:range :RestaurantRating ;
                 rdfs:comment "Relates a restaurant with his rating" ;
                 rdfs:label "hasRestaurantRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#takePlace
:takePlace rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Events
                                       :Hotels
                                       :PointsOfInteres
                                       :Restaurants
                                       :Transport
                                     )
                       ] ;
           rdfs:range :City ;
           rdfs:comment "Relates an entity with a city" ;
           rdfs:label "hasCity" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#transportAverageCost
:transportAverageCost rdf:type owl:ObjectProperty ;
                      rdfs:domain :Transport ;
                      rdfs:range :TransportAverageCost ;
                      rdfs:comment "Relates a transport with its average cost" ;
                      rdfs:label "hasTransportAverageCost" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#cost
:cost rdf:type owl:DatatypeProperty ;
      rdfs:domain :Cost ;
      rdfs:range xsd:decimal ;
      rdfs:comment "value of cost in euros" ;
      rdfs:label "hasCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:domain :Events ;
         rdfs:range xsd:date ;
         rdfs:comment "Date value of event" ;
         rdfs:label "hasDate" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#id
:id rdf:type owl:DatatypeProperty ;
    rdfs:domain :IdentifiableEntity ;
    rdfs:range xsd:integer ;
    rdfs:comment "Indicates the ID of the identity" ;
    rdfs:label "hasID" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#isExpensive
:isExpensive rdf:type owl:DatatypeProperty ;
             rdfs:domain :Cost ;
             rdfs:range xsd:boolean ;
             rdfs:comment "Indicate if the entity is expensive" ;
             rdfs:label "isExpensive" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#latitude
:latitude rdf:type owl:DatatypeProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:decimal ;
          rdfs:comment "Indicates latitude" ;
          rdfs:label "hasLatitude" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#longitude
:longitude rdf:type owl:DatatypeProperty ;
           rdfs:domain :City ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Indicates longitude" ;
           rdfs:label "hasLongitude" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :EntityWithName ;
      rdfs:range xsd:string ;
      rdfs:comment "Indicates the name" ;
      rdfs:label "hasName" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#needSunCream
:needSunCream rdf:type owl:DatatypeProperty ;
              rdfs:domain :PointsOfInteres ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Indicates if you need sun cream on the point of interest" ;
              rdfs:label "needSunCream" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:domain :Rating ;
        rdfs:range xsd:decimal ;
        rdfs:comment "Indicates the rating value" ;
        rdfs:label "hasRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#stars
:stars rdf:type owl:DatatypeProperty ;
       rdfs:domain :Hotels ;
       rdfs:range xsd:integer ;
       rdfs:comment "Indicates hotel stars" ;
       rdfs:label "hasStars" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :EntityWithName ;
      rdfs:comment "A city is a place that contain events, hotels, restaurants and points of interes" ;
      rdfs:label "City" ;
      rdfs:sameAs "<http://dbpedia.org/ontology/City>" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Cost
:Cost rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :cost ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:decimal
                      ] ;
      rdfs:comment "Cost represents a monetary value or price" ;
      rdfs:label "Cost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#EntityWithName
:EntityWithName rdf:type owl:Class ;
                rdfs:subClassOf :IdentifiableEntity ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :name ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] ;
                owl:disjointWith :Transport ;
                rdfs:comment "General class for entities that have a name property." ;
                rdfs:label "EntityWithName" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#EventTypes
:EventTypes rdf:type owl:Class ;
            rdfs:comment "Represents the category or classification of an event" ;
            rdfs:label "EventType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Events
:Events rdf:type owl:Class ;
        rdfs:subClassOf :EntityWithName ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasEventType ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :EventTypes
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :takePlace ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :City
                        ] ;
        rdfs:comment "A event represent a schedule activity that happen at a specific date and place" ;
        rdfs:label "Event" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#HotelNightCost
:HotelNightCost rdf:type owl:Class ;
                rdfs:subClassOf :Cost ;
                rdfs:comment "Night cost represent the price of a night in a hotel" ;
                rdfs:label "NightCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#HotelRating
:HotelRating rdf:type owl:Class ;
             rdfs:subClassOf :Rating ;
             rdfs:comment "A rating represents a qualitive evaluation of a hotel" ;
             rdfs:label "HotelRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#HotelType
:HotelType rdf:type owl:Class ;
           rdfs:comment "Represents the category or classification of a hotel" ;
           rdfs:label "HotelType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Hotels
:Hotels rdf:type owl:Class ;
        rdfs:subClassOf :EntityWithName ;
        rdfs:comment "A hotel is a place that provide lodging and other services to tourists" ;
        rdfs:label "Hotel" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#IdentifiableEntity
:IdentifiableEntity rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :id ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:integer
                                    ] ;
                    rdfs:comment "A general class for entities that can be uniquely identified by an ID." ;
                    rdfs:label "IdentifiableEntity" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#PointOfInterestRating
:PointOfInterestRating rdf:type owl:Class ;
                       rdfs:subClassOf :Rating ;
                       rdfs:comment "A rating represents a qualitive evaluation of a Point Of Interest" ;
                       rdfs:label "PointOfInterestRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#PointOfInterestTypes
:PointOfInterestTypes rdf:type owl:Class ;
                      rdfs:comment "Represents the category or classification of  a point of interest" ;
                      rdfs:label "PointOfInterestType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#PointsOfInteres
:PointsOfInteres rdf:type owl:Class ;
                 rdfs:subClassOf :EntityWithName ;
                 rdfs:comment "A Point of interest represents a location that may attract tourist" ;
                 rdfs:label "PointOfInterest" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Rating
:Rating rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :rating ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:decimal
                        ] ;
        rdfs:comment "A rating represents a qualitive evaluation" ;
        rdfs:label "Rating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#RestaurantAverageCost
:RestaurantAverageCost rdf:type owl:Class ;
                       rdfs:subClassOf :Cost ;
                       rdfs:comment "This value represent the average cost of a meal at the restaurant" ;
                       rdfs:label "RestaurantAverageCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#RestaurantRating
:RestaurantRating rdf:type owl:Class ;
                  rdfs:subClassOf :Rating ;
                  rdfs:comment "A rating represents a qualitive evaluation of a Restaurant" ;
                  rdfs:label "RestaurantRating" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#RestaurantTypes
:RestaurantTypes rdf:type owl:Class ;
                 rdfs:comment "Represents the category or classification of the food type in the restaurant" ;
                 rdfs:label "RestaurantType" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Restaurants
:Restaurants rdf:type owl:Class ;
             rdfs:subClassOf :EntityWithName ;
             rdfs:comment "Represents a place where food and beverages are prepared and served to customers" ;
             rdfs:label "Restaurant" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#Transport
:Transport rdf:type owl:Class ;
           rdfs:subClassOf :IdentifiableEntity ;
           rdfs:comment "Represent a mean of transportation within a city" ;
           rdfs:label "Transport" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#TransportAverageCost
:TransportAverageCost rdf:type owl:Class ;
                      rdfs:subClassOf :Cost ;
                      rdfs:comment "Represents the average cost of a transport per person for a single trip." ;
                      rdfs:label "TransportAverageCost" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#TransportCompanies
:TransportCompanies rdf:type owl:Class ;
                    rdfs:comment "Represent the differents transport companies." ;
                    rdfs:label "TransportCompany" .


###  http://www.semanticweb.org/ivan.guzman/ontologies/2025/3/smart-turism#TransportTypes
:TransportTypes rdf:type owl:Class ;
                rdfs:comment "Represents the category or classification of the mean transport." ;
                rdfs:label "TransportType" .


